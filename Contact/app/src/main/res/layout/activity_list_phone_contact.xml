<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ListPhoneContactActivity"
    android:orientation="vertical"
    android:gravity="center">

    <Button
        android:id="@+id/add_phone_contacts_button"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Add Phone Contact"/>

    <Button
        android:id="@+id/read_phone_contacts_button"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Read Phone Contact"/>

    <ListView
        android:id="@+id/display_phone_ocntacts_list_view"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">


    </ListView>



</LinearLayout>